{
  "name": "noDriveFrontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "dev": "vite",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "coverage": "vitest run --coverage",
